{
    "name": "@magnit/frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "lerna run build:lib --stream && cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "build:no-lib": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "clean": "rimraf build",
        "eject": "react-scripts eject",
        "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test --passWithNoTests"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/core": "^10.0.16",
        "@magnit/components": "^0.1.0",
        "@magnit/entities": "^0.1.0",
        "@magnit/icons": "^0.1.0",
        "@magnit/services": "^0.1.0",
        "@magnit/task-editor": "^0.1.0",
        "@magnit/template-editor": "^0.1.0",
        "@material-ui/core": "^4.3.2",
        "@material-ui/icons": "^4.2.1",
        "@material-ui/styles": "^4.3.3",
        "@reach/router": "^1.2.1",
        "@types/jest": "^24.0.18",
        "@types/lodash": "^4.14.138",
        "@types/reach__router": "^1.2.4",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/react-jss": "^8.6.4",
        "@types/react-loadable": "^5.5.1",
        "@types/uuid": "^3.4.5",
        "emotion-theming": "^10.0.14",
        "jss": "^9.8.7",
        "lodash": "^4.17.15",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-jss": "^8.6.1",
        "react-loadable": "^5.5.0",
        "react-scripts": "^3.1.1",
        "typescript": "^3.5.3",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "babel-plugin-emotion": "^10.0.16",
        "babel-plugin-import": "^1.12.1",
        "cross-env": "^5.2.0",
        "lerna": "^3.16.4"
    }
}
