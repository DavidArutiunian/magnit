{
    "name": "@magnit/template-editor",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": true,
    "scripts": {
        "clean": "rimraf dist",
        "fast:build": "cross-env FAST_BUILD_ENABLED=true yarn build:lib",
        "fast:watch": "cross-env FAST_BUILD_ENABLED=true yarn watch:lib",
        "build:lib": "concurrently -n webpack,tsc \"cross-env NODE_ENV=production webpack --mode production\" \"tsc --emitDeclarationOnly -p tsconfig.json\"",
        "watch:lib": "concurrently -n webpack,tsc \"webpack --mode development --watch\" \"tsc --emitDeclarationOnly -p tsconfig.json -w\""
    },
    "peerDependencies": {
        "@emotion/core": "^10.0.10",
        "@material-ui/core": "^4.0.1",
        "@material-ui/icons": "^4.0.1",
        "@types/ramda": "^0.26.9",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "uuid": "^3.3.2",
        "ramda": "^0.26.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-emotion": "^10.0.9",
        "fork-ts-checker-webpack-plugin": "^1.3.5",
        "hard-source-webpack-plugin": "^0.13.1",
        "thread-loader": "^2.1.2",
        "ts-loader": "^6.0.2",
        "typescript": "^3.5.1",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.3",
        "peer-deps-externals-webpack-plugin": "^1.0.4",
        "webpack-node-externals": "^1.7.2"
    }
}
